{
  "name": "server-portfolio",
  "version": "1.0.0",
  "description": "Server portfolio 2023",
  "main": "src/server.ts",
  "repository": "git remote add origin https://github.com/levanthoi/server-portfolio.git",
  "scripts": {
    "dev1": "concurrently \"npx tsc -w\" \"nodemon -q dist/server.js\"",
